{
  "name": "@remote-app/app",
  "version": "0.8.2",
  "description": "A transmission remote session app",
  "main": "expo-router/entry",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgalat/remote-app",
    "directory": "packages/app"
  },
  "scripts": {
    "dev": "EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo start",
    "dev:android": "npx expo start --android",
    "build:android:development": "npx eas build --platform android --local --profile development",
    "build:android:production": "npx eas build --platform android --local --profile production",
    "lint": "eslint --max-warnings=0 .",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@ctrl/magnet-link": "3.1.2",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@remote-app/transmission-client": "*",
    "@tanstack/react-query": "^5.59.13",
    "expo": "^52.0.7",
    "expo-application": "~6.0.1",
    "expo-asset": "~11.0.1",
    "expo-background-fetch": "~13.0.3",
    "expo-build-properties": "~0.13.1",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.2",
    "expo-document-picker": "~13.0.1",
    "expo-file-system": "~18.0.3",
    "expo-font": "~13.0.1",
    "expo-intent-launcher": "~12.0.1",
    "expo-linking": "~7.0.2",
    "expo-local-authentication": "~15.0.1",
    "expo-network": "~7.0.0",
    "expo-notifications": "~0.29.8",
    "expo-router": "~4.0.6",
    "expo-splash-screen": "~0.29.11",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "expo-task-manager": "~12.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0"
  },
  "overrides": {
    "react-native-actions-sheet@>0.9.0": {
      "react-native-gesture-handler": "~2.16.1"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-background-fetch"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "~5.3.3"
  }
}
